{
  "name": "function-plotter",
  "version": "4.5.3",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "clean": "rm -rf build",
    "build": "npm run clean && export SET NODE_OPTIONS=--openssl-legacy-provider && webpack --mode production && cp -R error build/ && cp -R src/images/icons build/images/icons",
    "test": "jest",
    "testWithCoverage": "nyc --reporter=lcov npm run test",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fang-lin/function-plotter.git"
  },
  "dependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@semantic-release/git": "^10.0.1",
    "@types/history": "^4.7.5",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "^5.1.19",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.5.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.0.0",
    "history": "^4.10.1",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^8.0.3",
    "lodash": "^4.17.19",
    "mathjs": "^14.5.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "semantic-release": "^22.0.0",
    "source-map-loader": "^0.2.4",
    "styled-components": "^5.3.3",
    "terser-webpack-plugin": "^4.2.3",
    "ts-loader": "^8.1.0",
    "typescript": "^4.5.4",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.3",
    "webpack-manifest-plugin": "^2.2.0",
    "worker-plugin": "^5.0.0"
  },
  "engines": {
    "npm": "10.9.2",
    "node": "20.18.1"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "preset": "conventionalcommits"
  }
}
